@model IEnumerable<Employee>

@{
    ViewBag.Title = "Employee List";
}

<table>
    <thead>
        <tr><th>ID</th><th>Name</th><th>Department</th></tr>
    </thead>
    @foreach (var employee in Model)
    {
        <tr>
            <td>@employee.Id</td>
            <td>@employee.Name</td>
            <td>@employee.Department</td>
        </tr>
    }
</table>

@section Scripts{
    <script src="~/js/CustomScript.js"></script> }

